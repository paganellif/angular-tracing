{
  "name": "tracing-zipkin-angular",
  "version": "0.0.3",
  "license": "Apache-2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "build:libs:core": "ng build --project core",
    "build:libs:zipkin": "ng build --project zipkin && find ./dist -type f -name '*.js' -print0",
    "build:libs:zipkin:prod": "ng build --project zipkin --configuration production && find ./dist -type f -name '*.js' -print0",
    "build:libs": "yarn build:libs:zipkin",
    "build:libs:prod": "yarn build:libs:zipkin:prod",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "affected": "./node_modules/.bin/nx affected",
    "release": "npm version patch && pushd libs/zipkin && npm version patch && yarn test && ng build --project zipkin && pushd dist/libs/zipkin && npm publish --access public ."
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@angular/animations": "12.2.16",
    "@angular/common": "12.2.16",
    "@angular/compiler": "12.2.16",
    "@angular/core": "12.2.16",
    "@angular/forms": "12.2.16",
    "@angular/platform-browser": "12.2.16",
    "@angular/platform-browser-dynamic": "12.2.16",
    "@angular/router": "12.2.16",
    "@ngrx/effects": "13.0.2",
    "@ngrx/entity": "13.1.0",
    "@ngrx/router-store": "13.0.2",
    "@ngrx/store": "13.0.2",
    "@ngrx/store-devtools": "13.0.2",
    "@nrwl/nx": "7.8.7",
    "@types/zipkin-instrumentation-fetch": "^0.11.7",
    "bulma": "0.9.3",
    "core-js": "2.6.12",
    "empty-module": "0.0.2",
    "express": "4.17.3",
    "ngrx-data": "6.1.0-beta.3",
    "rxjs": "7.5.5",
    "tslib": "^2.0.0",
    "url-parse": "1.5.10",
    "zipkin": "0.22.0",
    "zipkin-transport-http": "0.16.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.16",
