{
  "name": "tracing-zipkin-angular",
  "version": "0.0.2",
  "license": "Apache-2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "build:libs:core": "ng build --project core",
    "build:libs:zipkin": "ng build --project zipkin && find ./dist -type f -name '*.js' -print0",
    "build:libs:zipkin:prod": "ng build --project zipkin --prod && find ./dist -type f -name '*.js' -print0",
    "build:libs": "yarn build:libs:zipkin",
    "build:libs:prod": "yarn build:libs:zipkin:prod",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "affected": "./node_modules/.bin/nx affected",
    "release": "npm version patch && pushd libs/zipkin && npm version patch && yarn test && ng build --project zipkin && pushd dist/libs/zipkin && npm publish --access public ."
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@angular/animations": "9.1.13",
    "@angular/common": "9.1.13",
    "@angular/compiler": "9.1.13",
    "@angular/core": "9.1.13",
    "@angular/forms": "9.1.13",
    "@angular/platform-browser": "9.1.13",
    "@angular/platform-browser-dynamic": "9.1.13",
    "@angular/router": "9.1.13",
    "@ngrx/effects": "13.0.2",
    "@ngrx/entity": "13.0.2",
    "@ngrx/router-store": "13.0.2",
    "@ngrx/store": "13.0.2",
    "@ngrx/store-devtools": "13.0.2",
    "@nrwl/nx": "7.8.7",
    "@types/zipkin-instrumentation-fetch": "^0.11.7",
    "bulma": "0.9.3",
    "core-js": "2.6.12",
    "empty-module": "0.0.2",
    "express": "4.17.2",
    "ngrx-data": "6.1.0-beta.3",
    "rxjs": "6.6.7",
    "tslib": "2.3.1",
    "url-parse": "1.5.7",
    "zipkin": "0.22.0",
    "zipkin-transport-http": "0.16.0",
    "zone.js": "~0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.15",
    "@angular-devkit/build-ng-packagr": "~0.901.15",
    "@angular-devkit/schematics": "9.1.15",
    "@angular/cli": "9.1.15",
    "@angular/compiler-cli": "9.1.13",
    "@angular/language-service": "9.1.13",
    "@nrwl/schematics": "8.12.11",
    "@types/jasmine": "3.10.3",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "^17.0.0",
    "@types/url-parse": "1.4.8",
    "codelyzer": "^6.0.0",
    "concurrently": "7.0.0",
    "husky": "7.0.4",
    "jasmine-core": "4.0.0",
    "jasmine-marbles": "0.8.4",
    "jasmine-spec-reporter": "7.0.0",
    "json-server": "0.17.0",
    "json-server-reset": "1.3.2",
    "karma": "6.3.16",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "ng-packagr": "^9.0.0",
    "prettier": "2.5.1",
    "pretty-quick": "3.1.3",
    "protractor": "7.0.0",
    "ts-node": "10.5.0",
    "tslib": "2.3.1",
    "tslint": "5.20.1",
    "typescript": "3.8.3"
  },
